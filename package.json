{
    "name": "@elizaos/plugin-goat",
    "version": "0.1.8+build.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "workspace:*",
        "@goat-sdk/adapter-vercel-ai": "0.2.0",
        "@goat-sdk/core": "0.4.0",
        "@goat-sdk/plugin-erc20": "0.2.2",
        "@goat-sdk/plugin-kim": "0.1.2",
        "@goat-sdk/wallet-evm": "0.2.0",
        "@goat-sdk/wallet-viem": "0.2.0",
        "tsup": "8.3.5"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch"
    },
    "peerDependencies": {
        "whatwg-url": "7.1.0"
    },
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
        "EVM_PRIVATE_KEY": {
            "type": "string",
            "description": "EVM private key"
        },
        "EVM_PROVIDER_URL": {
            "type": "string",
            "description": "EVM provider URL"
        }
    }
}